          kv_showme.parent = null
          kv_showme_it.parent = null
          JS.uiShow ("#gamePanes")
          JS.uiShow ("#location")
          JS.setBackground ("white")
          JS.setCss ("#txtCommand", "outline:none;box-shadow:0px 0px 2px 2px gold;")
