  <function name="ageCheck"><![CDATA[
    ClearScreen
    JS.setForeground ("white")
    JS.setBackground ("red")
    JS.setCss ("body", "background:black")
    JS.uiHide ("#gamePanes")
    JS.uiHide ("#location")
